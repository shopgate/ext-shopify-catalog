{
  "version": "1.0.0",
  "id": "@shopgate/shopify-catalog",
  "components": [
  ],
  "configuration": {
    "shopifyShopAlias": {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.frontend.alias"
      }
    },
    "shopifyAccessToken": {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.shopifyAccessToken"
      }
    }
  }
}
